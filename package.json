{
  "name": "cagibi",
  "description": "Tiny asynchronous state management based on static data stitching",
  "type": "module",
  "version": "0.1.21",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "exports": {
    "require": "./lib/cagibi.cjs",
    "default": "./lib/cagibi.modern.js"
  },
  "main": "./lib/cagibi.cjs",
  "module": "./lib/cagibi.module.js",
  "umd:main": "./lib/cagibi.umd.js",
  "unpkg": "./lib/cagibi.umd.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib",
    "media"
  ],
  "keywords": [
    "state",
    "management",
    "async",
    "typescript",
    "nodejs"
  ],
  "dependencies": {
    "@paralleldrive/cuid2": "^1.0.1",
    "lodash.mergewith": "^4.6.2",
    "lodash.reduce": "^4.6.0",
    "minie": "0.0.14",
    "tslib": "^2.4.0"
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch --compress false",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@apify/eslint-config-ts": "^0.2.3",
    "@apify/tsconfig": "^0.1.0",
    "@types/jest": "^28.1.3",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/lodash.reduce": "^4.6.7",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^28.1.1",
    "microbundle": "^0.15.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gahabeen/cagibi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gahabeen/cagibi.git"
  }
}
